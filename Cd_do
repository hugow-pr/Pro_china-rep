
label variable zongfen "RIC"
label variable rjfen "aveRIC1"
label variable dwmjfen "aveRIC1"
label variable digitalindex "DE"
label variable lndigital "ln(DE)"
label variable chanyejiegou "structure" 
label variable chengshihualv "urban"
label variable capital3s "capital"
label variable fdiper1 "fdi"
label variable population "size"
label variable gdpper1 "gdp"
label variable budget "budget" 
label variable RD "Gov"
label variable financial "Fin"
label variable humancapital "HC"
label variable digitalinfras "DI"
label variable newcompany "newfirm"
label variable touzi "investment"
label variable wailaibishu1 "CRinvestment"
label variable vcshumu1 "VC"
label variable output1 "invention"
label variable zhuanli1 "patent"
label variable shangbiao1 "trademark"
label variable yjbw "the number of post offices per million in each city at the end of 1984"
label variable Internet "the number of Internet users in each city at the end of 1984"
label variable RG "location type"
label variable cp "administrative level type"


**********
*Table 2 Summary statistics
********** 
logout, save(mytable) word replace: ///
 tabstat zongfen rjfen  dwmjfen digitalindex chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras newcompany touzi1 output1 zhuanli1 shangbiao1, ///
  stats(count mean sd p50 min max) ///
   c(s) format(%6.4f)	
**********
*Table 3 Basic results for the impact of consumer digital engagement on regional innovation capacity
********** 

xtset id year
gen lndigital=ln(digitalindex)

   *-column (1)
reg zongfen lndigital,r
est sto m1

   *-column (2)
reg zongfen lndigital chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras,r 
est sto m2

   *-column (3)
reg zongfen lndigital chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras i.year,r 
est sto m3

   *-column (4)
reg zongfen lndigital chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras i.year i.procode,r
est sto m4

 *-Export to word
outreg2 [m1 m2 m3 m4] using Basic.rtf,stats(coef tstat) aster(coef) bdec(4)tdec(2) adds(adj_R2,e(r2_a),F,e(F)) replace

**********
*Table 4 Regression results of city fixed effects and instrumental variables
********** 

 *-instrumental variable
gen iv1=yjbw*l.Internet

   *-column (1)
xtreg zongfen lndigital chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras i.year,r fe
est sto m5

   *-column (2)
ivreg2 zongfen chanyejiegou chengshihualv capital3s fdiper1 population  gdpper1 budget RD financial humancapital  digitalinfras i.year (lndigital=iv1) ,r
est sto m6

 *-Export to word
outreg2 [m5 m6] using feIV.rtf,stats(coef tstat) aster(coef) bdec(4)tdec(2) adds(adj_R2,e(r2_a),F,e(F)) replace


